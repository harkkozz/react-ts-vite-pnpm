{
  "endOfLine": "auto",
  "semi": true,
  "bracketSpacing": true,
  "trailingComma": "none",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^config/(.*)$",
    "^router/(.*)$",
    "^store/(.*)$",
    "^pages/(.*)$",
    "^components/(.*)$",
    "^[./]"
  ],
  "importOrderCaseInsensitive": true,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderParserPlugins" : [],
  "plugins": ["@trivago/prettier-plugin-sort-imports"]
}
